DB_SA_PASSWORD=SUA_SENHA_AQUI

# JWT Authentication (m√≠nimo 32 caracteres)
JWT_SECRET_KEY=sua-chave-secreta-com-minimo-32-caracteres

# Frontend API URL (para build)
VITE_API_URL=http://localhost:5000/api
